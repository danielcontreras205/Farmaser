	<queryString>
		<![CDATA[SELECT
     ventas.`id` AS ventas_id,
     ventas.`Fecha` AS ventas_Fecha,
     persona.`Nombre` AS persona_Nombre,
     persona.`Primer_Apellido` AS persona_Primer_Apellido,
     ventas.`Total_Venta` AS ventas_Total_Venta,
     ventas.`Hora_Venta` AS ventas_Hora_Venta
FROM
     `persona` persona INNER JOIN `ventas` ventas ON persona.`cc` = ventas.`cc_persona`
WHERE
ventas.`Fecha` BETWEEN '$P!{Fachi_Inicio}' AND '$P!{FechaFinal}']]>
	</queryString>
