	<queryString>
		<![CDATA[SELECT
     ventas.`id` AS ventas_id,
     ventas.`Fecha` AS ventas_Fecha,
     ventas.`Total_Venta` AS ventas_Total_Venta,
     persona.`Nombre` AS persona_Nombre,
     persona.`Primer_Apellido` AS persona_Primer_Apellido,
     medicamentos.`id` AS medicamentos_id,
     medicamentos.`NombreComercial` AS medicamentos_NombreComercial,
     medicamentos.`precentacion` AS medicamentos_precentacion,
     medicamentos.`Precio_medicamento` AS medicamentos_Precio_medicamento,
     venta_medicamento.`Cantidad` AS venta_medicamento_Cantidad
FROM
     `ventas` ventas INNER JOIN `venta_medicamento` venta_medicamento ON ventas.`id` = venta_medicamento.`id_Ventas`
     INNER JOIN `persona` persona ON ventas.`cc_persona` = persona.`cc`
     INNER JOIN `medicamentos` medicamentos ON venta_medicamento.`id_medicamentos` = medicamentos.`id`
WHERE
     ventas.`id` = '$P!{CodigoFactura}']]>
